{
  "root":true,
  "parser":"@typescript-eslint/parser",
  "plugins":[
    "@typescript-eslint"
  ],
  "extends":[
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  "rules":{
    "indent":[
      2,
      "tab"
    ],
    "quotes":[
      2,
      "single"
    ],
    "linebreak-style":[
      2,
      "unix"
    ],
    "semi":[
      2,
      "always"
    ]
  },
  "env":{
    "node":true
  }
}